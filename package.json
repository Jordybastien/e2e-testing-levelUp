{
  "name": "oaf-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "test": "mocha tests",
    "coverage": "nyc --reporter=html --reporter=text npm run test",
    "cy:run": "cypress run --no-exit --browser chrome"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.18.2",
    "hbs": "~4.0.4",
    "morgan": "~1.9.1",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cypress": "^4.4.0",
    "imgur": "^0.3.1",
    "mocha": "^7.1.0",
    "mocha-eslint": "^6.0.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "puppeteer": "^2.1.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "semi": "warn",
      "eqeqeq": "warn",
      "curly": "warn",
      "no-bitwise": "warn"
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
